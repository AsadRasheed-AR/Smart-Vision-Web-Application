r='primary' />} \r\n              onToggle={this.handleToggle} \r\n              curState={this.state.btn3} \r\n              autoControl={this.state.btn3Auto}/>\r\n          </List>\r\n        </CardContent>\r\n      </Card>\r\n    )\r\n  }\r\n\r\n  render(){\r\n       return (\r\n        <div>\r\n          {this.getContent()}\r\n          <AlertDialogSlide openDialog={this.state.showDialog} closeDialog={this.closeDialog}/>\r\n          <CustomizedSnackbars state={this.state.showSnackBar} message=\"Wifi Module Not Responding , Please Check Connection !\"/>\r\n        </div>\r\n        \r\n       )\r\n  }\r\n  }\r\n\r\nexport default SimpleCard\r\n","E:\\Git_Repositories\\SmartMonitoringSystem_Fyp_Complete\\SMS_WebApp\\sms_webapp\\src\\components\\AppBar.js",[],"E:\\Git_Repositories\\SmartMonitoringSystem_Fyp_Complete\\SMS_WebApp\\sms_webapp\\src\\components\\DialogBox.js",[],"E:\\Git_Repositories\\SmartMonitoringSystem_Fyp_Complete\\SMS_WebApp\\sms_webapp\\src\\components\\ListItem.js",[],"E:\\Git_Repositories\\SmartMonitoringSystem_Fyp_Complete\\SMS_WebApp\\sms_webapp\\src\\components\\SnackBar.js",[],["157","158"],"E:\\Git_Repositories\\SmartMonitoringSystem_Fyp_Complete\\Submission_Files\\SMS_WebApp\\sms_webapp\\src\\index.js",[],"E:\\Git_Repositories\\SmartMonitoringSystem_Fyp_Complete\\Submission_Files\\SMS_WebApp\\sms_webapp\\src\\components\\App.js",[],"E:\\Git_Repositories\\SmartMonitoringSystem_Fyp_Complete\\Submission_Files\\SMS_WebApp\\sms_webapp\\src\\components\\Drawer.js",[],"E:\\Git_Repositories\\SmartMonitoringSystem_Fyp_Complete\\Submission_Files\\SMS_WebApp\\sms_webapp\\src\\components\\ConfigurationCard.js",["159","160","161"],"E:\\Git_Repositories\\SmartMonitoringSystem_Fyp_Complete\\Submission_Files\\SMS_WebApp\\sms_webapp\\src\\components\\LiveStreamCard.js",["162","163","164"],"E:\\Git_Repositories\\SmartMonitoringSystem_Fyp_Complete\\Submission_Files\\SMS_WebApp\\sms_webapp\\src\\components\\CurrentStatusCard.js",["165","166","167","168","16